
@{
    ViewData["Title"] = "Iccanobif";
}

<div class=" taustakuva">
    <div class=" teksti">
        <p class="sankari">Tarvitsen 7-numeroisen pinkoodin päästäkseni K.V.A.A.K-laserin ohjauspaneeliin.</p>
        <p>@ViewBag.Fibo</p>
        <form action="iccanobif" method="post" class="form-inline">
            <div class="form-group m-1">
                <input type="text" class="form-control" name="pinkoodi" placeholder="pinkoodi" />
            </div>
            <div class="form-group m-1">
                <input type="submit" class="btn btn-light" name="fibonappi" value="Anna koodi" />
                @*<input type="hidden" name="random" value="@i" />*@
            </div>
        </form>
    </div>
</div>
